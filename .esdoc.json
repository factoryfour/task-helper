{
    "source": "./src",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "manual": {
                    "files": [
                        "./manuals/manual_dp.md",
                        "./manuals/manual_to.md"
                    ]
                }
            }
        },
        {
            "name": "esdoc-unexported-identifier-plugin",
            "option": {
                "enable": true
            }
        }
    ]
}